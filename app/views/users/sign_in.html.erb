<h1>會員登入</h1>

<%= form_with(url: sessions_path , scope: "user" ) do |form|%>

<div>
    <%= form.label :account , "帳號" , autocomplete: "off" , autofocus: true , placeholder: "信箱或使用者名稱"%>
    <%= form.text_field :account %>
</div>

<div>
    <%= form.label :password , "密碼" %>
    <%= form.password_field :password %>
</div>

<%= form.submit "登入"%>

<% end %>